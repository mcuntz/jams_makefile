# Set aliases so that one can, for example, say ifort to invoke standard intel11 on eve
ifneq (,$(findstring $(compiler),gnu gfortran gcc))
    icompiler := gnu51
endif
ifneq (,$(findstring $(compiler),intel ifort ifort12))
    icompiler := intel12
endif
